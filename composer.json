{
    "name": "mcfedr/monolog-aws",
    "description": "A monolog handler to send logs to SNS or SES",
    "keywords": ["log", "monolog", "amazon", "aws", "sns", "ses"],
    "require": {
        "aws/aws-sdk-php": "~3.0",
        "monolog/monolog": "~1.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "theseer/phpdox": "~0.7",
        "phploc/phploc": "~2.0",
        "pdepend/pdepend": "~2.0",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~1.5",
        "sebastian/phpcpd": "~2.0"
    },
    "autoload": {
        "psr-4": {"Mcfedr\\Monolog\\": "src/Mcfedr/Monolog"}
    },
    "autoload-dev": {
        "psr-4": {"Mcfedr\\Monolog\\": "tests/Mcfedr/Monolog"}
    }
}
